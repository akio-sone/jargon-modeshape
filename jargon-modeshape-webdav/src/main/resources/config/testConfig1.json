 {
    "name" : "irodsRepo",
    "jndiName" : "",
    "workspaces" : {
        "predefined" : ["otherWorkspace"],
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : true
        }
    },
    "storage" : {
        "cacheConfiguration" : "config/infinispan.xml",
        "cacheName" : "persistentRepository",
        "binaryStorage" : {
            "type" : "file",
            "directory": "target/federation_repository/binaries",
            "minimumBinarySizeInBytes" : 999
        }
    },
    "externalSources" : {
        "irods" : {
             "irodsHost":"",
            "irodsPort":1247,
            "irodsZone":"",
            "irodsDefaultResource":"demoResc",
            "irodsUser":"",
            "irodsPassword":"",
            "irodsAuthType":"STANDARD",
            "classname" : "org.irods.jargon.modeshape.connector.IRODSWriteableConnector",
            "directoryPath" : "/    /home/    ",
            "projections" : [
                "default:/irodsGrid => /"
            ],
             "queryable" : false,
            "readOnly" : false,
            "addMimeTypeMixin" : false,
            "addAvus" : false,
             "cacheTtlSeconds" : 640,
            "exclusionPattern" : ".+[.]tmp$"
        }
    } ,
    "node-types" : ["config/irods.cnd"]
         
} 